[sub_resource type="Shader" id="Shader_ocean"]
code = "
shader_type spatial;
render_mode cull_disabled, depth_draw_opaque, specular_schlick_ggx, blend_mix;

uniform float wave_strength : hint_range(0.0, 2.0) = 0.5;
uniform float wave_frequency : hint_range(0.0, 10.0) = 1.5;
uniform float wave_speed : hint_range(0.0, 5.0) = 1.0;
uniform vec4 water_color : source_color = vec4(0.2, 0.6, 0.9, 0.8);
uniform float metallic : hint_range(0.0, 1.0) = 0.1;
uniform float roughness : hint_range(0.0, 1.0) = 0.3;

void vertex() {
    float wave1 = sin(VERTEX.x * wave_frequency + TIME * wave_speed) * wave_strength;
    float wave2 = cos(VERTEX.z * wave_frequency * 0.8 + TIME * wave_speed * 1.2) * wave_strength * 0.6;
    VERTEX.y += wave1 + wave2;
}

void fragment() {
    ALBEDO = water_color.rgb;
    ALPHA = water_color.a;
    METALLIC = metallic;
    ROUGHNESS = roughness;
}
"

